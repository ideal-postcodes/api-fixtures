{
  "name": "@ideal-postcodes/api-fixtures",
  "description": "api-fixtures",
  "repository": {
    "type": "git",
    "url": "git@github.com:ideal-postcodes/api-fixtures"
  },
  "version": "0.0.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test node_modules/.bin/nyc node_modules/.bin/mocha --opts mocha.opts",
    "lint": "node_modules/.bin/tslint -c tslint.json -p tsconfig.json lib/**/*.ts",
    "build": "npm run lint && node_modules/.bin/tsc",
    "coverage": "node_modules/.bin/nyc report --reporter=text-lcov | node_modules/.bin/coveralls || true",
    "generate": "node_modules/.bin/ts-node bin/generate.ts && npm run test && npm run docs",
    "docs": "./node_modules/.bin/typedoc --mode file --target ES3 --out docs/ --name 'Ideal Postcodes API Fixtures' --theme minimal --excludePrivate --hideGenerator --excludeExternals lib/",
    "prepare": "npm run test && npm run build"
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "nyc": {
    "include": [
      "lib/**/*.ts",
      "generate/**/*.ts"
    ],
    "exclude": [
      "dist/*",
      "**/*.d.ts",
      "generate/index.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "engines": {
    "node": ">=8.0"
  },
  "author": "Ideal Postcodes",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "~4.1.7",
    "@types/dotenv": "~6.1.0",
    "@types/lodash": "~4.14.120",
    "@types/mocha": "~5.2.5",
    "@types/nock": "~9.3.1",
    "@types/node": "~11.9.0",
    "@types/node-fetch": "~2.1.4",
    "@types/prettier": "~1.16.0",
    "chai": "~4.2.0",
    "coveralls": "~3.0.2",
    "dotenv": "~6.2.0",
    "lodash": "~4.17.11",
    "mocha": "~5.2.0",
    "nock": "10.0.6",
    "node-fetch": "~2.3.0",
    "nyc": "~13.3.0",
    "prettier": "~1.16.3",
    "source-map-support": "~0.5.10",
    "ts-node": "~8.0.2",
    "tslint": "~5.12.1",
    "typedoc": "~0.14.2",
    "typescript": "~3.3.1"
  },
  "dependencies": {}
}
